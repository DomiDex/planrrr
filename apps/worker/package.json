{
  "name": "@repo/worker",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint . --max-warnings 0",
    "check-types": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:reset": "docker compose down -v && docker compose up -d",
    "setup:dev": "sh scripts/setup-dev.sh",
    "setup:dev:windows": "powershell -ExecutionPolicy Bypass -File scripts/setup-dev.ps1",
    "db:generate": "prisma generate --schema=../../packages/database/prisma/schema.prisma",
    "db:push": "prisma db push --schema=../../packages/database/prisma/schema.prisma",
    "db:studio": "prisma studio --schema=../../packages/database/prisma/schema.prisma"
  },
  "dependencies": {
    "@repo/database": "workspace:*",
    "@repo/redis": "workspace:*",
    "@sentry/node": "^7.99.0",
    "axios": "^1.7.2",
    "bullmq": "^5.58.0",
    "dotenv": "^16.3.1",
    "form-data": "^4.0.4",
    "googleapis": "^157.0.0",
    "ioredis": "^5.7.0",
    "prom-client": "^15.1.0",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.3.0",
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@testcontainers/postgresql": "^11.5.1",
    "@testcontainers/redis": "^11.5.1",
    "@types/form-data": "^2.5.2",
    "@types/node": "^20.19.11",
    "@vitest/coverage-v8": "^2.1.8",
    "eslint": "^9.33.0",
    "msw": "2.7.2",
    "supertest": "^7.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.8"
  }
}